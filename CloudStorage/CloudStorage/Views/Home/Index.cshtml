@model CloudStorage.ViewModels.HomePageViewModel

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Cloud storage</title>
</head>
<body>
    <h2>Welcome</h2>

    <h3>@Model.Message</h3>

    <div>
        @foreach (var fileInfo in Model.FileInfos)
        {
            <div>
                <div>@fileInfo.Id</div>
                <div>@fileInfo.FileName</div>
                <div>@fileInfo.FileSizeInBytes</div>
            </div>
            <hr/>
        }
    </div>
</body>
</html>